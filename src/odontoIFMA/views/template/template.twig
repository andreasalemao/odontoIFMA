<!DOCTYPE html>
<html lang="pt-BR">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="author" content="Maykon Deykon, Vitor Queiroz, Gustavo Moreno, Roberto Júnior">
    <link rel="icon" href="{{ app.request.basepath }}/img/favicon.png"/>
    <title>OdontoIFMA</title>

    <!-- Bootstrap Core CSS -->
    <link rel="stylesheet" href="{{ app.request.basepath }}/bootstrap/css/bootstrap.min.css">

    <!--Datepicker CSS-->
    <link href="{{ app.request.basepath }}/bootstrap/plugins/bootstrap-datetimepicker/css/bootstrap-datetimepicker.min.css" rel="stylesheet">

    <!--Bootstrap Validator CSS-->
    <link href="{{ app.request.basepath }}/bootstrap/plugins/bootstrapvalidator/bootstrapValidator.css" rel="stylesheet" />

    <!-- Custom CSS -->
    <link href="{{ app.request.basepath }}/css/sb-admin.css" rel="stylesheet">

    <!-- Morris Charts CSS -->
    <link href="{{ app.request.basepath }}/css/morris.css" rel="stylesheet">

    <!-- Custom Fonts -->
    <link href="{{ app.request.basepath }}/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css">

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
    <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->

</head>

<body>

<div id="wrapper">

    <!-- Navigation -->
    {#[Nome da rota, ícone, texto de exibição]#}
    {% set navigation_bar = [['home','fa fa-fw fa-home', 'Home'], ['cadTipoOperador' ,'fa fa-fw fa-edit', 'Cadastro Tipo Operador'], ['cadTipoPaciente' ,'fa fa-fw fa-edit', 'Cadastro Paciente'], ['cadTipoCampus' ,'fa fa-fw fa-edit', 'Cadastro Campus']] %}
    {% set active_page = active_page|default('home') %}

    <nav class="navbar navbar-inverse navbar-fixed-top" role="navigation">
        <!-- Brand and toggle get grouped for better mobile display -->
        <div class="navbar-header">
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-ex1-collapse">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            <a class="navbar-brand" href="/">OdontoIFMA</a>
        </div>
        <!-- Top Menu Items -->
        <ul class="nav navbar-right top-nav">
            <li class="dropdown">
                <a href="#" class="dropdown-toggle" data-toggle="dropdown"><i class="fa fa-envelope"></i> <b
                            class="caret"></b></a>
                <ul class="dropdown-menu message-dropdown">
                    <li class="message-preview">
                        <a href="#">
                            <div class="media">
                                    <span class="pull-left">
                                        <img class="media-object" src="http://placehold.it/50x50" alt="">
                                    </span>

                                <div class="media-body">
                                    <h5 class="media-heading"><strong>John Smith</strong>
                                    </h5>

                                    <p class="small text-muted"><i class="fa fa-clock-o"></i> Yesterday at 4:32 PM</p>

                                    <p>Lorem ipsum dolor sit amet, consectetur...</p>
                                </div>
                            </div>
                        </a>
                    </li>
                    <li class="message-preview">
                        <a href="#">
                            <div class="media">
                                    <span class="pull-left">
                                        <img class="media-object" src="http://placehold.it/50x50" alt="">
                                    </span>

                                <div class="media-body">
                                    <h5 class="media-heading"><strong>John Smith</strong>
                                    </h5>

                                    <p class="small text-muted"><i class="fa fa-clock-o"></i> Yesterday at 4:32 PM</p>

                                    <p>Lorem ipsum dolor sit amet, consectetur...</p>
                                </div>
                            </div>
                        </a>
                    </li>
                    <li class="message-preview">
                        <a href="#">
                            <div class="media">
                                    <span class="pull-left">
                                        <img class="media-object" src="http://placehold.it/50x50" alt="">
                                    </span>

                                <div class="media-body">
                                    <h5 class="media-heading"><strong>John Smith</strong>
                                    </h5>

                                    <p class="small text-muted"><i class="fa fa-clock-o"></i> Yesterday at 4:32 PM</p>

                                    <p>Lorem ipsum dolor sit amet, consectetur...</p>
                                </div>
                            </div>
                        </a>
                    </li>
                    <li class="message-footer">
                        <a href="#">Read All New Messages</a>
                    </li>
                </ul>
            </li>
            <li class="dropdown">
                <a href="#" class="dropdown-toggle" data-toggle="dropdown"><i class="fa fa-bell"></i> <b
                            class="caret"></b></a>
                <ul class="dropdown-menu alert-dropdown">
                    <li>
                        <a href="#">Alert Name <span class="label label-default">Alert Badge</span></a>
                    </li>
                    <li>
                        <a href="#">Alert Name <span class="label label-primary">Alert Badge</span></a>
                    </li>
                    <li>
                        <a href="#">Alert Name <span class="label label-success">Alert Badge</span></a>
                    </li>
                    <li>
                        <a href="#">Alert Name <span class="label label-info">Alert Badge</span></a>
                    </li>
                    <li>
                        <a href="#">Alert Name <span class="label label-warning">Alert Badge</span></a>
                    </li>
                    <li>
                        <a href="#">Alert Name <span class="label label-danger">Alert Badge</span></a>
                    </li>
                    <li class="divider"></li>
                    <li>
                        <a href="#">View All</a>
                    </li>
                </ul>
            </li>
            <li class="dropdown">
                <a href="#" class="dropdown-toggle" data-toggle="dropdown"><i class="fa fa-user"></i> John Smith <b
                            class="caret"></b></a>
                <ul class="dropdown-menu">
                    <li>
                        <a href="#"><i class="fa fa-fw fa-user"></i> Profile</a>
                    </li>
                    <li>
                        <a href="#"><i class="fa fa-fw fa-envelope"></i> Inbox</a>
                    </li>
                    <li>
                        <a href="#"><i class="fa fa-fw fa-gear"></i> Settings</a>
                    </li>
                    <li class="divider"></li>
                    <li>
                        <a href="#"><i class="fa fa-fw fa-power-off"></i> Log Out</a>
                    </li>
                </ul>
            </li>
        </ul>
        <!-- Sidebar Menu Items - These collapse to the responsive navigation menu on small screens -->
        <div class="collapse navbar-collapse navbar-ex1-collapse">
            <ul class="nav navbar-nav side-nav">
                {% for item in navigation_bar %}
                    <li{% if item[0] == active_page %} class="active"{% endif %}>
                        <a href="{{ app.url_generator.generate(item[0]) }}"><i class="{{ item[1] }}"></i> {{ item[2] }}
                        </a></li>
                {% endfor %}
            </ul>
        </div>
        <!-- /.navbar-collapse -->
    </nav>

    <div id="page-wrapper">
        <div class="container-fluid">
            {% block header %}
                {% set pgAtual = 'Cadastro de tipo de operador' %}
                <!-- Page Heading -->
                <div class="row">
                    <div class="col-lg-12">
                        {% for item in navigation_bar %}
                            {% if item[0] == active_page %}
                                <h1 class="page-header">
                                    {{ item[2] }}
                                </h1>
                            {% endif %}
                        {% endfor %}

                        <ol class="breadcrumb">
                            <li>
                                <i class="fa fa-home"></i> <a href="/">Home</a>
                            </li>
                            <li class="active">
                                {% for item in navigation_bar %}
                                    {% if item[0] == active_page %}
                                        <i class="{{ item[1] }}"></i> {{ item[2] }}
                                    {% endif %}
                                {% endfor %}
                            </li>
                        </ol>
                    </div>
                </div>
            {% endblock %}
            {% block content %}
            {% endblock %}
        </div>
        <!-- /.container-fluid -->

    </div>
    <!-- /#page-wrapper -->

</div>
<!-- /#wrapper -->

<!-- jQuery -->
<script src="{{ app.request.basepath }}/js/jquery-min.js"></script>
<script src="{{ app.request.basepath }}/js/jquery-ui.min.js"></script>

<!-- Bootstrap Core JavaScript -->
<script src="{{ app.request.basepath }}/bootstrap/js/bootstrap.min.js"></script>

<!--Datepicker js-->
<script src="{{ app.request.basepath }}/bootstrap/plugins/bootstrap-datetimepicker/moment.js"></script>
<script src="{{ app.request.basepath }}/bootstrap/plugins/bootstrap-datetimepicker/bootstrap-datetimepicker.min.js"></script>
<script src="{{ app.request.basepath }}/bootstrap/plugins/bootstrap-datetimepicker/locales/bootstrap-datetimepicker.pt-BR.js"></script>

<!--Bootstrap Validator js-->
<script src="{{ app.request.basepath }}/bootstrap/plugins/bootstrapvalidator/bootstrapValidator.js"></script>

<!--jQuery Mask Plugin js-->
<script src="{{ app.request.basepath }}/js/jquery.mask.min.js"></script>

<!--DataTables js-->
<script src="{{ app.request.basepath }}/js/jquery.plugin.min.js"></script>
<script src="{{ app.request.basepath }}/js/jquery.dataTables.min.js"></script>
<script src="{{ app.request.basepath }}/js/dataTables.bootstrap.js"></script>

<!-- Scripts locais -->
<script src="{{ app.request.basepath }}/js/scripts.js"></script>
<script src="{{ app.request.basepath }}/js/formValidate.js"></script>

</body>
</html>
